package laboration1;

public class DStringTest {

	
	public static void main(String[] args) {
		DString dstring1 = new DString(1);
		DString dstring2 = new DString();
		
		dstring1.append('A');
		dstring2.append('B');
		dstring2.append('C');
		
		dstring1.append(dstring2);
		
		System.out.println(dstring1.toString());
		
	}
}
