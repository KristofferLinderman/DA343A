package laboration12;

import java.io.Serializable;

public class Expression implements Serializable {

	private static final long serialVersionUID = 718277167921726736L;
	private double nbr1, nbr2;
	private char operator;

	public Expression(double nbr1, double nbr2, char operator) {
		this.nbr1 = nbr1;
		this.nbr2 = nbr2;
		this.operator = operator;
	}

	public double getNbr1() {
		return nbr1;
	}

	public double getNbr2() {
		return nbr2;
	}

	public char getOperator() {
		return operator;
	}
	
	public String toString(){
		return String.valueOf(nbr1) + operator + nbr2;
	}

}
