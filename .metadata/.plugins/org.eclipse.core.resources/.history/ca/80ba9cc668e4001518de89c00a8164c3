package p1;

import p2.FileProducer;

public class IconProducerManager {

	private Buffer<IconProducer> buffer;

	public IconProducerManager(Buffer<IconProducer> buffer) {
		this.buffer = buffer;
	}
	
	public void addIconProducer(ArrayProducer iconProducer){
		buffer.put(iconProducer);
	}
	
	public void addIconProducer(FileProducer iconProducer){
		buffer.put(iconProducer);
	}

}
